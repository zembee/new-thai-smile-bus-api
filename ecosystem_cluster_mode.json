{
    "apps": [
      {
        "name": "tsb-web-api-cluster",
        "script": "dist/main.js",
        "instances": 4,
        "exec_mode": "fork",
        "increment_var": "PORT",
        "autorestart": true,
        "watch": false,
        "max_memory_restart": "1G",
        "error_file": "logs/errors.txt",
        "out_file": "logs/logs.txt",
        "env_production": {
          "NODE_ENV": "production",
          "PORT": 3000,
          "JWT_SEED": "secret"
        }
      }
    ]
  }